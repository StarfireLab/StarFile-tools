               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </HBox>
         </center>
         <top>
            <Button fx:id="button1" mnemonicParsing="false" onAction="#xmlToCodeOnAction" text="转换为code" BorderPane.alignment="CENTER" />
